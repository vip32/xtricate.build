    ViewBag.Title = "xtricate.build demo site";
}

<h2>@ViewBag.Message</h2>
<p>
    To learn more about xtricate.build visit <a href="https://github.com/vip32/xtricate.build" title="xtricate.build on github">xtricate.build on github</a>.
</p>
<ul>
