        <script type="text/javascript" src="@ViewBag.WorldtimeServerUrl/clocks/embed.js"></script>
        <script type="text/javascript" language="JavaScript">objNL = new Object; objNL.wtsclock = "wtsclock024.swf"; objNL.color = "000080"; objNL.wtsid = "NL"; objNL.width = 100; objNL.height = 100; objNL.wmode = "transparent"; showClock(objNL);</script>
    </li>
